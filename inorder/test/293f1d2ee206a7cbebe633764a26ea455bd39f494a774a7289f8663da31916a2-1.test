    auxiliary: |-
        void in_order_help(tree_t *tree, int *buffer, int offset){
            if (!tree) return;
            int newOffset = 0;
            in_order_help(tree->left, buffer, offset);
            if (tree->left) newOffset += tree->left->number_elements;
            *(buffer + offset + newOffset) = tree->value;
            newOffset++;
            if (tree->right) in_order_help(tree->right, buffer, offset+newOffset);
        }
    inorder: |-
        int *ret = malloc(1);
        if (!tree) return NULL;
        ret = malloc(tree->number_elements*sizeof(int));
        if (!ret) return NULL;
        in_order_help(tree, ret, 0);
        return ret;
