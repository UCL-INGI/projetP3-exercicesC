    dequeue: |4-
            letter_t *ret = q->front;
            if (ret == NULL) return NULL;
            q->front = ret->next;
            q->size--;
            ret->next = NULL;
            if (q->front == NULL) q->rear = NULL;
            return ret;
    priority_enqueue: |4-
            if (!letter) return -1;
            q->size++;
            letter->next = NULL;
            letter_t *runner = q->front;
            if (!runner){
                q->front = letter;
                q->rear = letter;
                return 0;
            }
            if (runner->priority < letter->priority){
                letter->next = q->front;
                q->front = letter;
                return 0;
            }
            while (runner->next){
                if (runner->next->priority < letter->priority){
                    letter->next = runner->next;
                    runner->next = letter;
                    return 0;
                }
                runner = runner->next;
            }
            q->rear = letter;
            runner->next = letter;
            return 0;
    process_letters: |4-
            letter_t *ret = dequeue(post_office_queue);
            while(ret){
                if (priority_enqueue(*(continents_queue + ret->continent), ret)) return -1;
                ret = dequeue(post_office_queue);
            }
            return 0;
