    filter: |4-
            if(!list) return;
            node_t *first = NULL;
            node_t *runner = list->first;
            node_t *builder = NULL;
            int size = 0;
            for (size_t i = 0; i < list->size; i++)
            {
                node_t *temp = NULL;
                if (f(runner->value)){
                    if (!first){
                        first = runner;
                        builder = first;
                    }else{
                        builder->next = runner;
                        builder = runner;
                    }
                    size++;
                }else{
                    temp = runner;
                }
                runner = runner->next;
                if (temp) free(temp);
            }
            if (builder){
                builder->next = NULL;
            }
            list->size = size;
            list->first = first;
    map: |-
        if (!list) return;
        node_t *runner = list->first;
        while (runner){
            runner->value = f(runner->value);
            runner = runner->next;
        }
