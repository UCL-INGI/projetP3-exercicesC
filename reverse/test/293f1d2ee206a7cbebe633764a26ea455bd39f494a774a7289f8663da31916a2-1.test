    auxiliary: |-
        node_t* reverseHelp(node_t* head, node_t *acc){
            if (!head) return acc;
            node_t *new = malloc(sizeof(node_t));
            if (!new) return NULL;
            new->val = head->val;
            new->next = acc;
            return reverseHelp(head->next, new);
        }
    reverse: return reverseHelp(head, NULL);
