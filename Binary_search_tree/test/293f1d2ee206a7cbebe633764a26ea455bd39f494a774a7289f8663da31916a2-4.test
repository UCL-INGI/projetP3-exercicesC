    add: |-
        if (!root) {
            struct node *n = (struct node *)malloc(sizeof(struct node));
            if (!n) {
                return NULL;
            }
            n->value = value;
            n->left = NULL;
            n->right = NULL;
            return n;
        }
        struct node *runner = root;
        struct node *parent = root;
        int is_left = 0;
        while (runner) {
            parent = runner;
            if (runner->value < value) {
                runner = runner->left;
                is_left = 1;
            } else if (runner->value > value) {
                runner = runner->right;
                is_left = 0;
            } else {
                return root;
            }
        }
        struct node *n = (struct node *)malloc(sizeof(struct node));
        if (!n) {
            return NULL;
        }
        n->value = value;
        n->left = NULL;
        n->right = NULL;
        if (is_left) {
            parent->left = n;
        } else {
            parent->right = n;
        }
        return root;
    search: |-
        if (!root) {
            return 0;
        }
        while (root) {
            if (root->value < value) {
                root = root->left;
            } else if (root->value > value) {
                root = root->right;
            } else {
                return 1;
            }
        }
        return 0;
