    add: |4-
            struct node *new = malloc(sizeof(struct node));
            if (!new) return NULL;
            new->value = value;
            new->left = NULL;
            new->right = NULL;
            if (!root) return new;
            struct node *runner = root;
            while (1){
                if (runner->value == value){
                    free(new);
                    break;
                }
                if (runner->value < value){
                    if (runner->right){
                        runner = runner->right;
                    }else{
                        runner->right = new;
                        break;
                    }
                }else{
                    if (runner->left){
                        runner = runner->left;
                    }else{
                        runner->left = new;
                        break;
                    }
                }
            }
            return root;
    search: |-
        while(root){
            if (root->value == value) return 1;
            if (root->value > value) root = root->left;
            if (root->value < value) root = root->right;
        }
        return 0;
