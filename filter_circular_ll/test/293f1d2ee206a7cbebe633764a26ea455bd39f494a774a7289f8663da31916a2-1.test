            if (!list || !predicate) return NULL;
            list_t *new = malloc(sizeof(list_t));
            new->size = 0;
            new->tail = NULL;
            if (!list->tail) return new;
            node_t *runner = list->tail->next;
            node_t *head = NULL;
            node_t *curr = NULL;
            for (size_t i = 0; i < list->size; i++)
            {
                if (predicate(runner->value)){
                    new->size++;
                    if (!head){
                        head = malloc(sizeof(node_t));
                        if (!head) return NULL;
                        head->value = runner->value;
                        head->next = NULL;
                        curr = head;
                    }else{
                        node_t *temp = malloc(sizeof(node_t));
                        if (!temp) return NULL;
                        temp->value = runner->value;
                        temp->next = NULL;
                        curr->next = temp;
                        curr = temp;
                    }
                }
                runner = runner->next;
            }
            if (curr){
                curr->next = head;
            }
            new->tail = curr;
            return new;
