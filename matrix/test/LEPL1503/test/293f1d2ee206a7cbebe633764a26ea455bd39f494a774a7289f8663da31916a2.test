            int **mat = malloc(n*sizeof(int *));
            if (!mat) return NULL;
            for (size_t i = 0; i < n; i++)
            {
                *(mat + i) = malloc(sizeof(int)*m);
                if (!*(mat+i)) return NULL;
            }
            for (size_t i = 0; i < n; i++)
            {
                for (size_t j = 0; j < m; j++)
                {
                    mat[i][j] = -1;
                }
            }
            node_t *runner = lst->first;
            int count = 0;
            while (runner){
                if (count >= n*m) break;
                mat[count/m][count%m] = runner->value;
                count++;
                runner = runner->next;
            }
            return mat;
